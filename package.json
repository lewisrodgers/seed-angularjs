{
  "name": "acn-angular",
  "version": "1.0.0",
  "description": "Accenture Cloud First starting point for Angularjs front-end UI development in es6",
  "license": "MIT",
  "scripts": {
    "start": "./node_modules/.bin/npm-run-all --parallel test build serv",
    "test": "./node_modules/.bin/karma start",
    "build": "./node_modules/.bin/webpack --watch",
    "serv": "./node_modules/.bin/webpack-dev-server --inline --hot",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "precommit-lint": "./node_modules/.bin/eslint -c .eslintrc.json app/src/**/*.js",
    "precommit-test": "./node_modules/.bin/karma start --single-run"
  },
  "pre-commit": [
    "precommit-msg",
    "precommit-lint",
    "precommit-test"
  ],
  "dependencies": {
    "angular": "^1.5.8",
    "angular-ui-router": "^0.3.2"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-latest": "^6.16.0",
    "eslint": "^3.10.0",
    "eslint-config-google": "^0.7.0",
    "eslint-loader": "^1.6.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "ng-annotate": "^1.2.1",
    "ng-annotate-loader": "^0.2.0",
    "npm-run-all": "^3.1.1",
    "pre-commit": "^1.1.3",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
